{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "author": {
    "name": [
        "Festus-Olaleye Oluwafisayomi Oluwaseunfunmi",
        "Salami Rasheed Amiola"
      ],
      "email": "festusolaleyef@gmail.com"
  },
  "description": "An app that analyses Video Footage and detects the objects",
  "type": "module",
  "main": "main.js",
  "homepage": "https://github.com/CommanderOutpost/video_analyser",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{js,ts,tsx,jsx,scss,html,md,json}\" --ignore-path .gitignore",
    "electron:build": "vite build && electron-builder -- --win",
    "backend": "cd backend_python && python main.py",
    "test:web": "concurrently \"npm run dev\" \"npm run backend\"",
    "start": "vite build && electron ."
  },
  "build": {
    "appId": "com.cctvanalyser.cctvanalyser",
    "productName": "Video Analyser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "backend_python/**/*"
    ],
    "extraResources": [
      {
        "from": "backend_python",
        "to": "backend_python"
      }
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "build/icons/logo.icns",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "build/icons/logo.ico",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "category": "Utility",
      "icon": "build/icons/logo.png",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.446.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shadcn": "^2.1.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@electron/remote": "^2.1.2",
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}
